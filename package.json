{
  "name": "highlight-bind9-config",
  "displayName": "highlight-bind9-config",
  "description": "Highlighting for ISC bind v9 configuration files.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bind9conf",
        "aliases": [
          "bind9 config",
          "bind9conf"
        ],
        "extensions": [
          ".conf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bind9conf",
        "scopeName": "bind9conf",
        "path": "./syntaxes/bind9conf.tmLanguage.json"
      }
    ]
  }
}
